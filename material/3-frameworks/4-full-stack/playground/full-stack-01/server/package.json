{
  "name": "@mono/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsx watch src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "node src/database/cli 'migration:generate src/database/migrations/$0 -d src/database'",
    "migration:create": "node src/database/cli 'migration:create src/database/migrations/$0 -d src/database'",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database",
    "migration:show": "npm run typeorm migration:show -- -d src/database",
    "migration:run": "tsx ../node_modules/typeorm/cli.js migration:run -d ./src/database",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tsx": "^3.13.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@trpc/server": "^10.41.0",
    "@types/lodash": "^4.14.200",
    "better-sqlite3": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.22.4"
  }
}
